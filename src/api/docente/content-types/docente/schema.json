{
  "kind": "collectionType",
  "collectionName": "docentes",
  "info": {
    "singularName": "docente",
    "pluralName": "docentes",
    "displayName": "Docente"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellido": {
      "type": "string",
      "required": true
    },
    "foto": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "alumnos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::alumno.alumno"
    },
    "users": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "docente"
    }
  }
}
